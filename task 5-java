    private void btndeleteActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here: 
         try{ 
        Class.forName("com.mysql.cj.jdbc.Driver"); 
            Connection conn = (Connection)DriverManager.getConnection("jdbc:mysql://localhost:3306/mysql?zeroDateTimeBehavior=CONVERT_TO_NULL" , "root" , "7UFTaj12"); 
            Statement st = conn.createStatement();
            String sq1 = "DELETE FROM student.student_information WHERE student_id = ?"; 
            PreparedStatement prt = conn.prepareStatement(sq1); 
            
             int student_id = Integer.parseInt(txtdelete.getText());
                prt.setInt(1, student_id);
                int rowsAffected = prt.executeUpdate();

                if (rowsAffected > 0) {
                    JOptionPane.showMessageDialog(null, "Record has been deleted successfully!");
                } else {
                    JOptionPane.showMessageDialog(null, "No record found with ID: " + student_id);
                }
            conn.close();
            txtdelete.setText("");
           } catch(Exception e) {
    JOptionPane.showMessageDialog(null, e);
}   
    }                                         

