    private void BTNREGISTERActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here: 
        // public void actionPerformed(ActionEvent e) {  
        int seatsLeftCourse1 = 50;
    int seatsLeftCourse2 = 40;
    int seatsLeftCourse3 = 30;
        int[] idCounter = {0, 0, 0};
                int selectedCount = 0; 
                 int selectedID = -1;
                if (RBTN1.isSelected()) {
                    selectedCount++; 
                    selectedID = ++idCounter[0]; 
                    seatsLeftCourse1--;
                }
                if (RBTN2.isSelected()) {
                    selectedCount++;
                    selectedID = ++idCounter[1];
                    seatsLeftCourse2--;
                }
                if (RBTN3.isSelected()) {
                    selectedCount++;
                    selectedID = ++idCounter[2];
                }
                TXTNO.setText(""+selectedCount);
                if (selectedID != -1) {
                    txtid.setText("" + selectedID); 
                    seatsLeftCourse3--;
                }
            
         TXTNO.setText(String.valueOf(selectedCount));
    txtid.setText(String.valueOf(selectedID)); 
   DefaultTableModel model = (DefaultTableModel) tbl.getModel();
    model.setRowCount(0); // Clear previous rows
    model.addRow(new Object[]{"Course 1", seatsLeftCourse1});
    model.addRow(new Object[]{"Course 2", seatsLeftCourse2});
    model.addRow(new Object[]{"Course 3", seatsLeftCourse3});
    int student_id = Integer.parseInt(txtid.getText());
    String student_name = txtname.getText(); // Assuming jTextField1 holds the name
    int registered_courses = Integer.parseInt(TXTNO.getText()); // Assuming TXTNO holds the number of courses

    // JDBC connection parameters
    String url = "jdbc:mysql://localhost:3306/mysql?zeroDateTimeBehavior=CONVERT_TO_NULL";
    String username = "root";
    String password = "7UFTaj12";

    // SQL INSERT query
    try{ 
            Class.forName("com.mysql.cj.jdbc.Driver"); 
            Connection con = (Connection)DriverManager.getConnection("jdbc:mysql://localhost:3306/mysql?zeroDateTimeBehavior=CONVERT_TO_NULL" , "root" , "7UFTaj12"); 
            String sq1= "INSERT INTO student.student_information values (?,?,?)";
            PreparedStatement prt = con.prepareStatement(sq1);
            prt.setString(1,txtid.getText());
            prt.setString(2,txtname.getText());
            prt.setString(3,TXTNO.getText()); 
            
            prt.executeUpdate();
            JOptionPane.showMessageDialog(null, "student registered successfully!");
            con.close(); 
            txtid.setText(""); txtname.setText(""); TXTNO.setText(""); 
        } catch(Exception e) {
            JOptionPane.showMessageDialog(null, e);
        }

    }                                           

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here: 
         coursesRegistration obj = new coursesRegistration();
