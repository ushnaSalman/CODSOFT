    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here: 
        try{ 
            Class.forName("com.mysql.cj.jdbc.Driver"); 
            Connection conn = (Connection)DriverManager.getConnection("jdbc:mysql://localhost:3306/mysql?zeroDateTimeBehavior=CONVERT_TO_NULL" , "root" , "7UFTaj12"); 
            Statement st = conn.createStatement();
            String sq1 = "SELECT * FROM course.course_information ";
            PreparedStatement prt = conn.prepareStatement(sq1); 
            ResultSet rs = prt.executeQuery();
            DefaultTableModel dt = (DefaultTableModel)jTable1.getModel();
            dt.setRowCount(0); 
            while(rs.next()) {
            Object[] o = { 
            rs.getString("course_code"),
            rs.getString("title"),
            rs.getString("description"),
            rs.getString("capacity")
            }; 
            dt.addRow(o);
            }
            
        }catch(Exception e) {
            JOptionPane.showMessageDialog(null, e); }
    }                                        

